{
  "family": "vlnstempleweb-task",
  "containerDefinitions": [
    {
      "name": "vlnstempleweb-container",
      "image": "IMAGE_URI",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8085,
          "hostPort": 8085
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::479547938101:role/ecsTaskExecutionRole"
}