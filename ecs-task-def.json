{
      "family": "vlnstempleweb-task",
      "containerDefinitions": [
        {
          "name": "vlnstempleweb-container",
          "image": "IMAGE_URI",
          "memory": 512,
          "cpu": 256,
          "essential": true,
          "portMappings": [
            {
              "containerPort": 8085,
              "hostPort": 8085
            }
          ],
      "environment": [
        {
          "name": "aws.secretName",
          "value": "prod/app/credentials"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/vlnstempleweb",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
      "requiresCompatibilities": ["FARGATE"],
      "networkMode": "awsvpc",
      "cpu": "256",
      "memory": "512",
      "executionRoleArn": "arn:aws:iam::479547938101:role/ecsTaskExecutionRole"
    }